#!/bin/bash

######################
# Run correct script #
######################
case "$1" in
  cd)
    script=hub_cd
    ;;
  db)
    script=hub_db
    ;;
  errors)
    script=hub_errors
    ;;
  permissions)
    script=hub_ogre_permissions
    ;;
  *)
    script=hub_set_env
    ;;
esac

. ~/.bin/$script "$@"
