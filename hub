#!/bin/bash

#########################
# Set hub env variables #
#########################
. ~/.bin/hub_set_env

######################
# Run correct script #
######################
case "$1" in
  cd)
    . ~/.bin/hub_cd "$@"
    ;;
  errors)
    . ~/.bin/hub_errors "$@"
    ;;
  mysql)
    . ~/.bin/hub_mysql "$@"
    ;;
esac
